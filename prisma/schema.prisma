generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PortRequest {
  id             String   @id @default(cuid())
  userId         String
  firstName      String
  lastName       String
  address        String
  city           String
  state          String
  zip            String
  accountNumber  String
  transferPin    String
  status         String   @default("pending")
  createdAt      DateTime @default(now())
}

